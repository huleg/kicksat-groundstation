<?xml version="1.0"?>
<block>
  <name>Python Decoder</name>
  <key>sprite_sprite_decoder_f</key>
  <category>Sprite</category>
  <import>import sprite</import>
  <make>sprite.sprite_decoder_f($snr_threshold)</make>

  <param>
    <name>SNR Threshold</name>
    <key>snr_threshold</key>
    <value>4.0</value>
    <type>real</type>
  </param>

  <sink>
    <name>in</name>
    <type>float</type>
  </sink>

</block>
